{
    "name": "ppokatilo/jms-deyaml-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle adding YAML deserialization to the JMS serializer bundle",
    "keywords": [ "deserialization", "yaml" , "jms", "symfony" ],
    "homepage": "http://github.com/SHyx0rmZ/jms-deyaml-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Pokatilo",
            "email": "mail@shyxormz.net"
        }
    ],
    "require": {
        "php": ">=5.4",
        "jms/serializer-bundle": "^1.1",
        "symfony/config": "^2.0|^3.0",
        "symfony/dependency-injection": "^2.0|^3.0",
        "symfony/http-kernel": "^2.0|^3.0",
        "ppokatilo/jms-deyaml": "^1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "symfony/framework-bundle": "^2.0|^3.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "SHyx0rmZ\\JMSDeYamlBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SHyx0rmZ\\JMSDeYamlBundle\\Test\\": "tests/"
        }
    }
}
